<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.vinicius.app10_cadastro_de_devs">

    <application
            android:allowBackup="true"
            android:icon="@drawable/floppy_disk"
            android:label="@string/app_name"
            android:roundIcon="@drawable/floppy_disk"
            android:supportsRtl="true"
            android:theme="@style/Theme.App10_Cadastro_de_Devs">
        <activity android:name=".CadastroActivity"
                  android:label="@string/titulo_cadastro">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>